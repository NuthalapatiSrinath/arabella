{
    "buildCommand": "cd arabella-frontendd-main && npm install && npm run build",
    "outputDirectory": "arabella-frontendd-main/build",
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/images/(.*)",
            "dest": "/images/$1"
        },
        {
            "src": "/videos/(.*)",
            "dest": "/videos/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}
